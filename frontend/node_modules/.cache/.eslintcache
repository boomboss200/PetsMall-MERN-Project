Items.map((item) => (\n                <ListGroup.Item key={item._id}>\n                  <Row className=\"align-items-center\">\n                    <Col md={4} style={{marginTop:'3%'}}>\n                      <img\n                        src={item.image}\n                        alt={item.name}\n                        className=\"img-fluid rounded img-thumbnail\"\n                      ></img>{' '}\n                      <Link to={`/product/${item.slug}`} style={{color:'hsl(214, 32%, 39%)',fontWeight:'bold',textDecoration:'none'}}>{item.name}</Link>\n                    </Col>\n                    <Col md={3}>\n                      <Button\n                        onClick={() =>\n                          updateCartHandler(item, item.quantity - 1)\n                        }\n                        variant=\"light\"\n                        disabled={item.quantity === 1}\n                      >\n                        <i className=\"fas fa-minus-circle\"></i>\n                      </Button>{' '}\n                      <span>{item.quantity}</span>{' '}\n                      <Button\n                        variant=\"light\"\n                        onClick={() =>\n                          updateCartHandler(item, item.quantity + 1)\n                        }\n                        disabled={item.quantity === item.countInStock}\n                      >\n                        <i className=\"fas fa-plus-circle\"></i>\n                      </Button>\n                    </Col>\n                    <Col md={3}>Rs{item.price}</Col>\n                    <Col md={2}>\n                      <Button\n                        onClick={() => removeItemHandler(item)}\n                        variant=\"light\"\n                      >\n                        <i className=\"fas fa-trash\"></i>\n                      </Button>\n                    </Col>\n                  </Row>\n                </ListGroup.Item>\n                \n              ))}\n              <div style={{paddingBottom:'10%',paddingTop:'2%'}}>\n                <LinkContainer to={'/shop'} style={{textAlign:'center',alignItems:'center',alignContent:'center', width: \"30%\", height: \"50px\",marginLeft:'40%',marginTop:'3%',fontWeight:'bold',fontSize:18,backgroundColor:' hsl(214, 32%, 39%)',color:'white'}}>\n                 <Button style={{marginLeft:'12%'}}> <BiArrowBack/> &nbsp;&nbsp;CONTINUE SHOPPING</Button>\n                </LinkContainer>\n                </div>\n            </ListGroup>\n          )}\n        </Col>\n        <Col md={4}>\n          <Card style={{marginTop:'6%'}}>\n            <Card.Body>\n              <ListGroup variant=\"flush\">\n                <ListGroup.Item>\n                  <h3>\n                    Subtotal ({cartItems.reduce((a, c) => a + c.quantity, 0)}{' '}\n                    items) : Rs\n                    {cartItems.reduce((a, c) => a + c.price * c.quantity, 0)}\n                  </h3>\n                </ListGroup.Item>\n                <ListGroup.Item>\n                  <div className=\"d-grid\">\n                    <Button\n                      type=\"button\"\n                      style={{backgroundColor:'#dd3333'}}\n                      variant=\"danger\"\n                      onClick={checkoutHandler}\n                      disabled={cartItems.length === 0}\n                    >\n                      Proceed to Checkout\n                    </Button>\n                  </div>\n                </ListGroup.Item>\n              </ListGroup>\n            </Card.Body>\n          </Card>\n        </Col>\n      </Row>\n    </div>\n  );\n}\n","/Users/Behzad/Documents/GitHub/PetsMall-MERN-Project/Petsmall-finalPatch/frontend/src/screens/LoginScreen.js",[],"/Users/Behzad/Documents/GitHub/PetsMall-MERN-Project/Petsmall-finalPatch/frontend/src/screens/OrderHistoryScreen.js",[],"/Users/Behzad/Documents/GitHub/PetsMall-MERN-Project/Petsmall-finalPatch/frontend/src/screens/SearchScreen.js",[],"/Users/Behzad/Documents/